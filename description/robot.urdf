<?xml version="1.0" ?>
<robot name="robot">
    <!-- Arm Links -->
    <link name="world" />
    <link name="dummy">
        <visual>
            <geometry>
                <sphere radius="0.01" />
            </geometry>
            <material name="absplastic">
                <color rgba="1 1 1 1" />
            </material>
        </visual>
        <collision>
            <geometry>
                <sphere radius="0.01" />
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0.16015982 0.02373002 0" />
            <mass value="1.08815625" />
            <inertia ixx="0.00252530" ixy="0.00023020" ixz="0.00000000" iyy="0.00412432" iyz="0.00000000" izz="0.00208426" />
        </inertial>
    </link>
    <link name="base">
        <visual>
            <geometry>
                <mesh filename="package://basic_velocity_control/description/models/base.stl" />
            </geometry>
            <material name="absplastic">
                <color rgba="1 1 1 1" />
            </material>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://basic_velocity_control/description/models/base.stl" />
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0.20908008" />
            <mass value="27.291" />
            <inertia ixx="0.79951627" ixy="0.00000007" ixz="0.00000003" iyy="0.79951627" iyz="0.00000000" izz="0.26227900" />
        </inertial>
    </link>
    <link name="upperarm">
        <visual>
            <geometry>
                <mesh filename="package://basic_velocity_control/description/models/upperarm.stl" />
            </geometry>
            <material name="absplastic">
                <color rgba="1 1 1 1" />
            </material>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://basic_velocity_control/description/models/upperarm.stl" />
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0.11286363 0.05317309 0" />
            <mass value="1897.69177373" />
            <inertia ixx="0.66854846" ixy="-0.23746553" ixz="0.00000000" iyy="1.61631302" iyz="0.00000000" izz="2.00530601" />
        </inertial>
    </link>
    <link name="forearm">
        <visual>
            <geometry>
                <mesh filename="package://basic_velocity_control/description/models/forearm.stl" />
            </geometry>
            <material name="absplastic">
                <color rgba="1 1 1 1" />
            </material>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://basic_velocity_control/description/models/forearm.stl" />
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0.11286363 0.05317309 0" />
            <mass value="36.13666037" />
            <inertia ixx="0.66854846" ixy="-0.23746553" ixz="0.00000000" iyy="1.61631302" iyz="0.00000000" izz="2.00530601" />
        </inertial>
    </link>
    <link name="hand">
        <visual>
            <geometry>
                <mesh filename="package://basic_velocity_control/description/models/hand.stl" />
            </geometry>
            <material name="absplastic">
                <color rgba="1 1 1 1" />
            </material>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://basic_velocity_control/description/models/hand.stl" />
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0.01715536 0 0" />
            <mass value="10.32823427" />
            <inertia ixx="0.10297083" ixy="0.00000000" ixz="0.00000000" iyy="0.05951089" iyz="0.00000000" izz="0.09362682" />
        </inertial>
    </link>
    <!-- Gripper links -->
    <link name="finger1">
        <visual>
            <geometry>
                <mesh filename="package://basic_velocity_control/description/models/finger1.stl" />
            </geometry>
            <material name="absplastic">
                <color rgba="1 1 1 1" />
            </material>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://basic_velocity_control/description/models/finger1.stl" />
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0.16015982 0.02373002 0" />
            <mass value="1.08815625" />
            <inertia ixx="0.00252530" ixy="0.00023020" ixz="0.00000000" iyy="0.00412432" iyz="0.00000000" izz="0.00208426" />
        </inertial>
    </link>
    <link name="finger2">
        <visual>
            <geometry>
                <mesh filename="package://basic_velocity_control/description/models/finger2.stl" />
            </geometry>
            <material name="aluminum">
                <color rgba="1 1 1 1" />
            </material>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://basic_velocity_control/description/models/finger2.stl" />
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0.16015982 0.02373002 0" />
            <mass value="1.08815625" />
            <inertia ixx="0.00252530" ixy="0.00023020" ixz="0.00000000" iyy="0.00412432" iyz="0.00000000" izz="0.00208426" />
        </inertial>
    </link>
    <!-- Arm joints -->
    <joint name="base_joint" type="revolute">
        <origin rpy="0 0 0" xyz="0 0 0" />
        <axis xyz="0 0 1" />
        <parent link="world" />
        <child link="base" />
        <limit effort="100000" velocity="1.0" lower="-1.5708" upper="1.5708" />
        <dynamics damping="0.1" friction="1.4" />
    </joint>
    <joint name="shoulder_joint" type="revolute">
        <origin rpy="0 0 0" xyz="0 0 0.670" />
        <axis xyz="0 1 0" />
        <parent link="base" />
        <child link="upperarm" />
        <limit effort="100000" velocity="1.0" lower="-0.8417" upper="1.5708" />
        <dynamics damping="0.1" friction="1.4" />
    </joint>
    <joint name="elbow_joint" type="revolute">
        <origin rpy="0 0 0" xyz="0.7 0 0" />
        <axis xyz="0 1 0" />
        <parent link="upperarm" />
        <child link="forearm" />
        <limit effort="100000" velocity="1.0" lower="0" upper="1.5708" />
        <dynamics damping="0.7" friction="1.4" />
    </joint>
    <joint name="wrist_joint" type="revolute">
        <origin rpy="0 0 0" xyz="0.7 0.05 0.0" />
        <axis xyz="1 0 0" />
        <parent link="forearm" />
        <child link="hand" />
        <limit effort="100000" velocity="1.0" lower="-1.5708" upper="1.5708" />
        <dynamics damping="0.7" friction="1.4" />
    </joint>
    <joint name="finger_joint" type="prismatic">
        <origin rpy="0 0 0" xyz="0 0 0" />
        <axis xyz="0 1 0" />
        <parent link="hand" />
        <child link="dummy" />
        <limit effort="100000" velocity="1.0" lower="0" upper="0.07" />
        <dynamics damping="0.7" friction="1.4" />
    </joint>
    <joint name="finger1_joint" type="prismatic">
        <origin rpy="0 0 0" xyz="0 0 0" />
        <axis xyz="0 1 0" />
        <parent link="hand" />
        <child link="finger1" />
        <limit effort="100000" velocity="1.0" lower="0" upper="0.07" />
        <dynamics damping="0.7" friction="1.4" />
        <mimic joint="finger_joint" multiplier="1.0" offset="0.0" />
    </joint>
    <joint name="finger2_joint" type="prismatic">
        <origin rpy="0 0 0" xyz="0 0 0" />
        <axis xyz="0 1 0" />
        <parent link="hand" />
        <child link="finger2" />
        <limit effort="100000" velocity="1.0" lower="-0.07" upper="0" />
        <dynamics damping="0.7" friction="1.4" />
        <mimic joint="finger_joint" multiplier="-1.0" offset="0.0" />
    </joint>
    <!-- Transmissions -->
    <transmission name="trans_base_joint">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="base_joint">
            <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
        </joint>
        <actuator name="base_joint_motor">
            <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>
    <transmission name="trans_shoulder_joint">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="shoulder_joint">
            <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
        </joint>
        <actuator name="shoulder_joint_motor">
            <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>
    <transmission name="trans_elbow_joint">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="elbow_joint">
            <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
        </joint>
        <actuator name="elbow_joint_motor">
            <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>
    <transmission name="trans_wrist_joint">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="wrist_joint">
            <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
        </joint>
        <actuator name="wrist_joint_motor">
            <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>
    <transmission name="trans_finger_joint">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="finger_joint">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
        </joint>
        <actuator name="finger_joint_motor">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>
    <transmission name="trans_finger1_joint">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="finger1_joint">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
        </joint>
        <actuator name="finger1_joint_motor">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>
    <transmission name="trans_finger2_joint">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="finger2_joint">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
        </joint>
        <actuator name="finger2_joint_motor">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>
    <!-- Plugins -->
    <gazebo>
        <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
            <robotNamespace>/</robotNamespace>
        </plugin>
    </gazebo>
</robot>

